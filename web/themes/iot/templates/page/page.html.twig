<!--menu mobile-->
{% if page.menumobile %}
      {% block menumobile %}
        {{ page.menumobile }}
      {% endblock %}
{% endif %}
<!--header-->
{% include '@iot/include/header.html.twig' %}
<div class="page">
  <!--menu-->
  {% if page.menubar %}
    <nav class="menu hidden-sm hidden-xs">
      <div class="container">
        {% block menubar %}
          {{ page.menubar }}
        {% endblock %}
      </div>
    </nav>
  {% endif %}
  <div class="container">
    <div class=" {% if page.sidebar_right %}cols {% else %} col {% endif %}">
      <div class="colleft">
          {{ page.breadcrumb }}
          {% if page.help %}
              {% block help %}
                  {{ page.help }}
              {% endblock %}
          {% endif %}

          {# Content #}
          {% block content %}
              {{ page.content }}
          {% endblock %}
      </div>
        {# Sidebar Second #}
        {% if page.sidebar_right %}
            {% block sidebar_right %}
              <div class="colright hidden-sm hidden-xs">
                  {{ page.sidebar_right }}
              </div>
            {% endblock %}
        {% endif %}

    </div>
  </div>
    {# Help #}

</div>
<!--footer-->
{% if page.footer %}
    {% block footer %}
      <footer class="footer {{ container }}" role="contentinfo">
          {{ page.footer }}
      </footer>
    {% endblock %}
{% endif %}
</div>

{% if page.footer_hidden%}
    {% block footer_hidden %}
      {{ page.footer_hidden }}
    {% endblock %}
{% endif %}